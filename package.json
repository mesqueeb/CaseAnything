{
  "name": "CaseAnything",
  "version": "0.1.8",
  "type": "module",
  "scripts": {
    "test": "swift test --enable-swift-testing",
    "bump": "np --no-cleanup --no-publish",
    "postversion": "replace-regex --from=\"\\d+\\.\\d+\\.\\d+\" --to=\"$npm_package_version\" README.md && git add README.md && git commit -m \"chore: bump\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "np": "^10.2.0",
    "replace-regex": "^0.0.4"
  },
  "funding": "https://github.com/sponsors/mesqueeb",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/CaseAnything.git"
  },
  "homepage": "https://github.com/mesqueeb/CaseAnything#readme",
  "bugs": "https://github.com/mesqueeb/CaseAnything/issues"
}
